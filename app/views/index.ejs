            });

            $('#showLabels').click(function(e) {
                  var atoms = $3Dmol.viewers['viewer_3Dmoljs'].getModel().selectedAtoms({atom:'CA'});
                  for ( var a in atoms) {
                      var atom = atoms[a];

                      var l = $3Dmol.viewers['viewer_3Dmoljs'].addLabel(atom.resn + " " + atom.resi, {
                          inFront : true,
                          fontSize : 12,
                          position : {
                            x : atom.x,
                            y : atom.y,
                            z : atom.z
                          }
                      });
                      atom.label = l;
                  }
                  $3Dmol.viewers['viewer_3Dmoljs'].render();
            });

            $('#hideLabels').click(function(e) {
                  var atoms = $3Dmol.viewers['viewer_3Dmoljs'].getModel().selectedAtoms({atom:'CA'});
                  for ( var a in atoms) {
                      var atom = atoms[a];
                      $3Dmol.viewers['viewer_3Dmoljs'].removeLabel(atom.label);
