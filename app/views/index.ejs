    <script type="text/javascript">
        $(document).ready(function () {

            var unique_id = $.gritter.add({
                // (string | mandatory) the heading of the notification
                title: 'Welcome to CyMol!',
                // (string | mandatory) the text inside the notification
                text: 'Hover me to enable the Close Button. You can hide the left sidebar clicking on the button next to the logo.',
                // (string | optional) the image to display on the left
                image: './img/ui-fjossinet.jpg',
                // (bool | optional) if you want it to fade out on its own or just sit there
                sticky: true,
                // (int | optional) the time you want it to be alive for before fading out
                time: '',
                // (string | optional) the class name you want to apply to that specific message
                class_name: 'my-sticky-class'
            });

            var getAllAsSelection = function() {
              var chains = [];
                $("#chain option").each(function() {
                    if ($(this).val() != "Chain" && $(this).val() != "All")
                      chains.push($(this).val());
                });
                selection = {'chain': chains};
                return selection;
            }

            var getCurrentSelection = function() {
              var selection = undefined;
              if ($("#chain").val() == "All") {
                var chains = [];
                $("#chain option").each(function() {
                    if ($(this).val() != "Chain" && $(this).val() != "All")
                      chains.push($(this).val());
                });
                selection = {'chain': chains};
              } else {
                selection = {'chain': $("#chain").val()};
                if ($("#selectFrom").val() != null && $("#selectFrom").val() != "" && $("#selectTo").val() != null && $("#selectTo").val() != "") {
                  var resi = [];
                  for (var i = parseInt($("#selectFrom").val()) ; i <= parseInt($("#selectTo").val()) ; i++)
                    resi.push(i);
                  selection['resi'] = resi;
                }
              }
              return selection;
            }

            var socket = io();
            var ladda = undefined;

            socket.on('gotPDB', function(pdbId, data, chains) {
                $3Dmol.viewers['viewer_3Dmoljs'].clear();
                $3Dmol.viewers['viewer_3Dmoljs'].addModel(data,'pdb');

                $3Dmol.viewers['viewer_3Dmoljs'].zoomTo();
                $3Dmol.viewers['viewer_3Dmoljs'].render();

                $("#chain").find('option').remove().end().append('<option selected disabled>Chain</option>');
                $("#chain").append('<option>All</option>')
                for (var i = 0 ; i < chains.length; i++)
                  $("#chain").append('<option>'+chains[i]+'</option>');
                if (ladda != undefined)
                  ladda.stop();
            });

            $('#loadPDB').click(function(e) {
                e.preventDefault();
                if (ladda == undefined)
                  ladda = Ladda.create(this);
                ladda.start();
                $3Dmol.viewers['viewer_3Dmoljs'].clear();
                socket.emit('loadPDB', $("#pdbId").val());
            });

            $('#showHighlight').click(function(e) {
                if ($("#chain").val() != undefined) {
                  $3Dmol.viewers['viewer_3Dmoljs'].removeAllSurfaces();
                  $3Dmol.viewers['viewer_3Dmoljs'].addSurface($3Dmol.SurfaceType.VDW, $3Dmol.specStringToObject("opacity:.7;color:green"), getCurrentSelection(), getCurrentSelection());
                  $3Dmol.viewers['viewer_3Dmoljs'].render();
                }
            });

            $('#hideHighlight').click(function(e) {
                $3Dmol.viewers['viewer_3Dmoljs'].removeAllSurfaces();
                $3Dmol.viewers['viewer_3Dmoljs'].render();
            });

            $('#showSelection').click(function(e) {
                if ($("#chain").val() != undefined) {
                  $3Dmol.viewers['viewer_3Dmoljs'].setStyle(getCurrentSelection(), $3Dmol.specStringToObject("line"));
